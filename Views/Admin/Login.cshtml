						<div class="sign-in-form-top">
							<h1><b style="color:red">Admin </b>Log in</h1>
						</div>
						<div class="signin">
							<form method="post" action="/Admin/Admin">
							<div class="log-input">
								<div class="log-input-left">
								   <input type="text" name="name" class="user" value="Your Email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Your Email';}"/>
								</div>
								<div class="clearfix"> </div>
							</div>
							<div class="log-input">
								<div class="log-input-left">
								   <input type="password" name="pass" class="lock" value="password" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email address:';}"/>
								</div>
								<div class="clearfix"> </div>
							</div>
							<input type="submit" value="Log in">
						</form>	 
						</div>
					</div>
				</div>
			</div>
